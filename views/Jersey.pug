extends layout

block content
  h1= title

div(class="container-fluid")
  div(class="row")
    - let results = [
        {name: "Jersey_type"}, 
        {name: "size"}, 
        {name: "Jersey_number"}
        ]
    - each item in results
      div(class="col-sm-2") #{item.name}:
        div(class="col-sm-2") #{item.name}:
          div(class="col-sm-2") #{item.name}:
